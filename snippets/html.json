{
  "wr-if": {
    "prefix": "wr-if",
    "body": [
      "<wr-if condition=\"$1\">",
      "\t<wr-then>",
      "\t\t$2",
      "\t</wr-then>",
      "\t<wr-else>",
      "\t\t$3",
      "\t</wr-else>",
      "</wr-if>"
    ],
    "description": "WebRelease if-then-else",
    "scope": "text.html"
  },
  "wr-ifc": {
    "prefix": "wr-ifc",
    "body": [
      "<wr-if condition=\"$1\">",
      "\t$2",
      "</wr-if>"
    ],
    "description": "WebRelease if statement",
    "scope": "text.html"
  },
  "wr-switch": {
    "prefix": "wr-switch",
    "body": [
      "<wr-switch value=\"$1\">",
      "\t<wr-case value=\"$2\">",
      "\t\t$3",
      "\t</wr-case>",
      "\t<wr-default>",
      "\t\t$4",
      "\t</wr-default>",
      "</wr-switch>"
    ],
    "description": "WebRelease switch statement",
    "scope": "text.html"
  },
  "wr-case": {
    "prefix": "wr-case",
    "body": [
      "<wr-case value=\"$1\">",
      "\t$2",
      "</wr-case>"
    ],
    "description": "WebRelease case",
    "scope": "text.html"
  },
  "wr-conditional": {
    "prefix": "wr-conditional",
    "body": [
      "<wr-conditional>",
      "\t<wr-cond condition=\"$1\">",
      "\t\t$2",
      "\t</wr-cond>",
      "\t<wr-cond condition=\"true\">",
      "\t\t$3",
      "\t</wr-cond>",
      "</wr-conditional>"
    ],
    "description": "WebRelease conditional",
    "scope": "text.html"
  },
  "wr-cond": {
    "prefix": "wr-cond",
    "body": [
